OCESQL*            SELECT HEBERGEMENT_ID, HEBERGEMENT_ID_ACTIVITE,
OCESQL*                   HEBERGEMENT_TYPE, HEBERGEMENT_NBNUIT
OCESQL*            FROM HEBERGEMENT
OCESQL*            WHERE HEBERGEMENT_ID_ACTIVITE = :WS-ACT-ID
OCESQL*        END-EXEC
OCESQL     CALL "OCESQLStartSQL"
OCESQL     END-CALL
OCESQL     CALL "OCESQLSetSQLParams" USING
OCESQL          BY VALUE 1
OCESQL          BY VALUE 9
OCESQL          BY VALUE 0
OCESQL          BY REFERENCE WS-ACT-ID
OCESQL     END-CALL
OCESQL     CALL "OCESQLCursorDeclareParams" USING
OCESQL          BY REFERENCE SQLCA
OCESQL          BY REFERENCE "DAL_CHEB" & x"00"
OCESQL          BY REFERENCE SQ0006
