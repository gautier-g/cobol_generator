{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "CobolSmartGen compile report",
  "type": "object",
  "additionalProperties": true,
  "required": ["results"],
  "properties": {
    "results": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["file", "rc", "stdout", "stderr", "started_at", "duration_ms"],
        "additionalProperties": true,
        "properties": {
          "file": { "type": "string" },
          "rc": { "type": "integer" },
          "stdout": { "type": "string" },
          "stderr": { "type": "string" },
          "started_at": { "type": "string" },
          "duration_ms": { "type": "number" }
        }
      }
    }
  }
}
